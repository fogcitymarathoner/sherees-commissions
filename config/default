import os

if os.environ.get('AWS_ACCESS_KEY_ID'):
    AWS_ACCESS_KEY_ID = os.environ.get('AWS_ACCESS_KEY_ID')
else:
    print('Environment Variable AWS_ACCESS_KEY_ID not set')
    quit(1)

if os.environ.get('AWS_SECRET_ACCESS_KEY'):
    AWS_SECRET_ACCESS_KEY = os.environ.get('AWS_SECRET_ACCESS_KEY')
else:
    print('Environment Variable AWS_SECRET_ACCESS_KEY not set')
    quit(1)

if os.environ.get('AWS_BUCKET'):
    AWS_BUCKET = os.environ.get('AWS_BUCKET')
else:
    print('Environment Variable AWS_BUCKET not set')
    quit(1)

if os.environ.get('RRG_PORT'):
    RRG_PORT = os.environ.get('RRG_PORT')
else:
    print('Environment Variable RRG_PORT not set')
    quit(1)

if os.environ.get('DATADIR'):
    DATADIR = os.environ.get('DATADIR')
else:
    print('Environment Variable DATADIR not set')
    quit(1)

if os.environ.get('MYSQL_SERVER_PORT_3306_TCP_ADDR'):
    MYSQL_SERVER_PORT_3306_TCP_ADDR = os.environ.get('MYSQL_SERVER_PORT_3306_TCP_ADDR')
else:
    print('Environment Variable MYSQL_SERVER_PORT_3306_TCP_ADDR not set')
    quit(1)

if os.environ.get('MYSQL_SERVER_PORT_3306_TCP_PORT'):
    MYSQL_SERVER_PORT_3306_TCP_PORT = os.environ.get('MYSQL_SERVER_PORT_3306_TCP_PORT')
else:
    print('Environment Variable MYSQL_SERVER_PORT_3306_TCP_PORT not set')
    quit(1)

if os.environ.get('DB'):
    DB = os.environ.get('DB')
else:
    print('Environment Variable DB not set')
    quit(1)

if os.environ.get('MYSQL_PASS'):
    MYSQL_PASS = os.environ.get('MYSQL_PASS')
else:
    print('Environment Variable MYSQL_PASS not set')
    quit(1)

if os.environ.get('MYSQL_USER'):
    MYSQL_USER = os.environ.get('MYSQL_USER')
else:
    print('Environment Variable MYSQL_USER not set')
    quit(1)

if os.environ.get('MYSQL_DB'):
    MYSQL_DB = os.environ.get('MYSQL_DB')
else:
    print('Environment Variable MYSQL_DB not set')
    quit(1)

if os.environ.get('KEYZCAR_DIR'):
    KEYZCAR_DIR = os.environ.get('KEYZCAR_DIR')
else:
    print('Environment Variable KEYZCAR_DIR not set')
    quit(1)


